                                                primary\r\n                                                onClick={() =>\r\n                                                    handleClick(\r\n                                                        editUser({\r\n                                                            user_id,\r\n                                                            username,\r\n                                                            password,\r\n                                                        })\r\n                                                    )\r\n                                                }\r\n                                            />\r\n                                        )}\r\n                                    />\r\n                                </Table.Cell>\r\n                            </Table.Row>\r\n                        );\r\n                    })}\r\n                </Table.Body>\r\n                <Table.Footer fullWidth>\r\n                    <Table.Row>\r\n                        <Table.HeaderCell colSpan='4'>\r\n                            <Form.Group>\r\n                                <Form.Button\r\n                                    type='button'\r\n                                    icon\r\n                                    content='Done'\r\n                                    floated='right'\r\n                                    secondary\r\n                                    size='huge'\r\n                                    onClick={() => history.push('/')}\r\n                                />\r\n                                <Form.Button\r\n                                    floated='right'\r\n                                    icon\r\n                                    type='button'\r\n                                    labelPosition='left'\r\n                                    primary\r\n                                    size='huge'\r\n                      